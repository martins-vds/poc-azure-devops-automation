{
  "function_key": {
    "type": "String",
    "value": "@appsetting('FUNCTION_KEY')"
  },
  "function_url": {
    "type": "String",
    "value": "@appsetting('FUNCTION_URL')"
  },
  "azurequeues_queueName": {
    "type": "String",
    "value": "@appsetting('AZUREQUEUES_QUEUENAME')"
  },
  "azurequeues_storageAccountName": {
    "type": "String",
    "value": "@appsetting('AZUREQUEUES_STORAGEACCOUNTNAME')"
  },
  "azurequeues_authentication": {
    "type": "object",
    "value": {
      "type": "ManagedServiceIdentity"
    }
  },
  "azurequeues_connectionProperties": {
    "type": "object",
    "value": {
      "authentication": {
        "audience": "https://storage.azure.com/",
        "type": "ManagedServiceIdentity"
      }
    }
  }
}