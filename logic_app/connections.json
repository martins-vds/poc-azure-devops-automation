      },
      "authentication": "@parameters('azurequeues_authentication')",
      "connectionProperties": "@parameters('azurequeues_connectionProperties')",
      "connectionRuntimeUrl": "@appsetting('BLOB_CONNECTION_RUNTIMEURL')"
    }
  }
}
